{
  "version": "0.7.1",
  "proposedName": "Scheduled And Recurring Payments Snap",
  "description": "An example snap for storing address book entries.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Montoya/address-book-snap-tutorial.git"
  },
  "source": {
    "shasum": "bd/U57gTsDv6U/ekvEi2p96BTadzLra7v9nFNH/NEYE=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@metamask/address-book-snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:network-access": {},
    "snap_manageState": {},
    "endowment:long-running": {},
    "endowment:transaction-insight": {
      "allowTransactionOrigin": true
    },
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "checkTransaction",
            "params": {
              "param1": "bar"
            }
          }
        },
        {
          "expression": "* * * * *",
          "request": {
            "method": "recurringTransaction",
            "params": {
              "param1": "foo"
            }
          }
        }
      ]
    },
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    }
  },
  "manifestVersion": "0.1"
}
